         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Label>
      <Label fx:id="points" layoutX="14.0" layoutY="131.0" text="Label" />
      <Label layoutX="13.0" layoutY="169.0" text="Prijs in badges">
